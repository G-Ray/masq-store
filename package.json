{
  "name": "masq-store",
  "version": "1.0.1",
  "description": "Data store for Masq",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm dist/index.js dist/sync.js dist/store.js dist/util.js dist/permissions.js",
    "build": "./node_modules/.bin/babel ./src --experimental -d ./dist && browserify ./dist/index.js --standalone MasqStore > dist/masqstore.js && npm run clean",
    "minify": "npm run build && uglifyjs ./dist/masqstore.js -o ./dist/masqstore.min.js --compress --mangle -- ",
    "dist": "npm run minify",
    "start": "npm run dist && http-server -p 8081."
  },
  "author": {
    "name": "Andrei Sambra",
    "email": "a.sambra@qwant.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^14.5.0",
    "http-server": "^0.10.0",
    "uglify-js": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QwantResearch/masq-store.git"
  },
  "dependencies": {
    "masq-crypto": "0.0.1",
    "url": "^0.11.0"
  }
}

    "url": "git+https://github.com/QwantResearch/masq-store.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "localforage": "^1.7.1",
    "masq-common": "git+https://github.com/QwantResearch/masq-common.git"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "browserify": "^16.1.1",
    "jest": "^22.3.0"
  }
}

{
  "name": "masq-hub",
  "version": "0.0.1",
  "description": "Storage hub for Masq",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm dist/index.js",
    "build": "./node_modules/.bin/babel ./src --experimental -d ./dist && browserify ./dist/index.js --standalone MasqHub > dist/hub.js && npm run clean",
    "minify": "npm run build && uglifyjs ./dist/hub.js -o ./dist/hub.min.js --compress --mangle -- ",
    "dist": "npm run minify",
    "start": "npm run dist && http-server ."
  },
  "author": {
    "name": "Andrei Sambra",
    "email": "a.sambra@qwant.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^14.5.0",
    "http-server": "^0.10.0",
    "uglify-js": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QwantResearch/masq-hub.git"
  }
}
